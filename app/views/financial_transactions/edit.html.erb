<div class="dashboard-container">
<%= form_with(model: @transaction, local: true, class: "form-data") do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :amount %>
  <%= form.number_field :amount %>

  <%= form.hidden_field :group_id %>

  <%= form.submit "Update Transaction", class: "button" %>
<% end %>
